#include "minishell.h"

void	ft_free_lexer(t_splitor **x)
{
	t_splitor	*tmp;

	while (*x != NULL)
	{
		tmp = *x;
		*x = (*x)->next;
		free(tmp->in);
		free(tmp);
	}
}

void	ft_free_env(t_envarment **my_env)
{
	t_envarment	*tmp;

	while (*my_env != NULL)
	{
		tmp = *my_env;
		*my_env = (*my_env)->next;
		free(tmp);
	}
}

void	ft_free_command(t_command *lst)
{
	t_command	*tmp;
	int			i;

	while (lst != NULL)
	{
		tmp = lst;
		lst = lst->next;
		i = 0;
		while (tmp->arg[i] != NULL)
		{
			free(tmp->arg[i]);
			i++;
		}
		free(tmp->arg);
		if (tmp->doc != NULL)
		{
			free(tmp->doc);
		}
		free(tmp);
	}
}
