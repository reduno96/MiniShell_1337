#include "../minishell.h"

void 	ft_env( t_envarment *var)
{
	t_envarment *ptr;
	ptr = var;
	while (ptr)
	{
		printf("%s <<<>> %s \n", ptr->var, ptr->data);
		ptr=ptr->next;
	}
}
