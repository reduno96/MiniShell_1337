#include "../minishell.h"


void 	ft_unset(t_envarment *var , t_command *list)
{
	t_envarment *env, *env_1;
	env = var;
	env_1 = var;
	int i=1;

	while (list->arg[i])
	{
			while (env_1)
			{
				if (ft_strcmp(env_1->var , list->arg[i]) == 0)
				{
					env->next = env_1->next;
					free(env_1);
				}
				env = env_1;
				env_1 = env_1->next;
			}
		env = var;
		env_1 = var;
		i++;
	}
}/* ************************************************************************** */
